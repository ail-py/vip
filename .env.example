# Copy to .env and fill values (do NOT commit real secrets)
# Cloudflare account ID (UUID)
CF_ACCOUNT_ID=01234567-89ab-cdef-0123-456789abcdef

# D1 Database id (UUID), if using D1. If not using, remove the D1 block from wrangler.toml
D1_DATABASE_ID=01234567-89ab-cdef-0123-456789abcdef

# Optional admin secret used by the worker
ADMIN_SECRET=your-strong-secret
